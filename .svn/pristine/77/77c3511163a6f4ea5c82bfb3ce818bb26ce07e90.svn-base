<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
 	 <meta charset='utf-8'>
 	 <meta http-equiv='X-UA-Compatible' content='IE=edge'>
     <meta name='viewport' content='width=device-width, initial-scale=1'>
     <link rel="stylesheet" href="styleguide.css" />
     <link rel="stylesheet" href="../css/style.css" />
</head>
<body>
  <div class="signup">
    <div class="div">
      <div class="text-wrapper">Login</div>
      <div class="ellipse"></div>
      <div class="group">
        <div class="overlap-group">
          <p class="element-APCOM-all-ri">© 2022 Dramatic. All Rights Reserved.</p>
          <div class="text-wrapper-2">이용약관</div>
          <div class="text-wrapper-3">문의하기</div>
          <div class="text-wrapper-4">공지사항</div>
          <div class="text-wrapper-5">공지사항</div>
          <img class="line" src="img/line-5.svg" />
        </div>
      </div>
      <div class="frame">
        <div class="frame-2">
          <div class="frame-3">
            <div class="upper-section">
              <div class="login-text">
                <div class="text-wrapper-6">Signup</div>
                <p class="p">Just some details to get you in.!</p>
              </div>
              <form class="credentials">
                  <input class="div-wrapper" type="text" id="userid" name="userid" placeholder="userid">
                  <input class="div-wrapper" type="text" id="username" name="username" placeholder="Username">
                  <input class="div-wrapper" type="text" id="nickname" name="nickname" placeholder="nickname">
                  <input class="div-wrapper" type="text" id="email" name="email" placeholder="Email">
                <div class="password-rem">
                  <input class="password" placeholder="Password" name="password" type="password" id="password" />
                  <input class="password" placeholder="Confirm Password" name="passwordchk" type="password" id="passwordchk" />
                </div>
                <div class="login-bt-fp">
                  <button class="login text-wrapper-8">Signup</button>
                </div>
              </form>
            </div>
            <div class="other-logins"><div class="or"></div></div>
          </div>
          <div class="customer-care-wrapper"><div class="customer-care"></div></div>
        </div>
      </div>
      <div class="navbar">
        <div class="top">
          <div class="text-wrapper-9">Moving</div>
          <img class="search" src="img/search.svg" />
        </div>
        <img class="rectangle" src="img/rectangle-4.svg" />
        <div class="ellipse-2"></div>
        <div class="text-wrapper-10">한국영화</div>
        <div class="text-wrapper-11">외국영화</div>
        <div class="text-wrapper-12">게시판</div>
        <div class="text-wrapper-13">home</div>
      </div>
    </div>
  </div>
</body>
</html>
