<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.mpago.mapper.CastMapper">
	<insert id="insert">
		insert into tbl_cast(title, rate, runningTime, mdate, rated, language, outline, status)
		values (#{title}, #{rate}, #{runningTime}, #{mdate}, #{rated}, #{language}, #{outline}, #{status})
	</insert>
	
	<select id="select" resultType="kr.co.mpago.domain.Cast">
	</select>	
	<delete id="delete" >
	</delete>
	<update id="update">
	</update>
	<select id="selectList" resultType="kr.co.mpago.domain.Cast">
	</select>
</mapper>